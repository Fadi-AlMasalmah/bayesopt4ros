<launch>
  <arg name="objective" default="myFun"/>
  <param name="objective" value="$(arg objective)"/>

  <arg name="bayesopt_config" default="$(find bayesopt4ros)/test/integration/configs/Contextual_myFun.yaml"/>
  <param name="bayesopt_config" value="$(arg bayesopt_config)"/>

  <node name="ContextBayesOpt" pkg="bayesopt4ros" type="contextual_bayesopt_node.py" output="screen" required="true"/>

  <node name="MyClientConetxtualNode" pkg="bayesopt4ros" type="myClient_contextualV0.0.py" output="screen"/>
  <!-- <test test-name="TestClientPython" pkg="bayesopt4ros" type="test_client_python.py"/> -->
</launch>
